.fade-enter-active
  transition all 0.3s!important
.fade-leave-active
  transition all 0.3s!important
.fade-enter-from,
.fade-leave-to
  opacity 0
.fade-enter-to,
.fade-leave-from
  opacity 1
.fade-slow-enter-active
  transition all 0.8s!important
.fade-slow-leave-active
  transition all 0.8s!important
.fade-slow-enter-from,
.fade-slow-leave-to
  opacity 0
.fade-slow-enter-to,
.fade-slow-leave-from
  opacity 1
